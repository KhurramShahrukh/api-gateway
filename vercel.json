{
  "version": 2,
  "name": "api-gateway",
  "builds": [
    {
      "src": "gateway/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "gateway/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "gateway/server.js": {
      "maxDuration": 30
    }
  }
}
